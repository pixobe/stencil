{
  "name": "@pixobe/stencil-components",
  "version": "0.0.4",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/stencil-components/stencil-components.esm.js",
      "require": "./dist/stencil-components/stencil-components.cjs.js"
    },
    "./dropdown-menu": {
      "import": "./dist/components/dropdown-menu.js",
      "types": "./dist/components/dropdown-menu.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./icon-move": {
      "import": "./dist/components/icon-move.js",
      "types": "./dist/components/icon-move.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pixobe/stencil-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "^4.14.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.20",
    "@stencil/sass": "^3.0.10",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/html": "^8.0.0",
    "@storybook/html-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0",
    "storybook": "^8.0.0"
  },
  "license": "MIT"
}
